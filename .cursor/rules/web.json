{
    "web": {
        "frontend": {
            "framework": "vue.js",
            "structure": {
                "required_directories": [
                    "src/components",
                    "src/views",
                    "src/assets",
                    "src/store",
                    "src/router"
                ],
                "naming": {
                    "components": "^[A-Z][a-zA-Z0-9]*\\.vue$",
                    "views": "^[A-Z][a-zA-Z0-9]*\\.vue$",
                    "store_modules": "^[a-z][a-z0-9]*\\.js$"
                }
            },
            "style": {
                "css_framework": "tailwindcss",
                "responsive_design": true,
                "theme": {
                    "primary_color": "#4F46E5",
                    "secondary_color": "#10B981"
                }
            }
        },
        "backend": {
            "framework": "spring-boot",
            "structure": {
                "required_directories": [
                    "src/main/java",
                    "src/main/resources",
                    "src/test/java"
                ],
                "package_structure": "com.imageprocess",
                "layers": [
                    "controller",
                    "service",
                    "repository",
                    "model",
                    "config"
                ]
            },
            "api": {
                "versioning": "v1",
                "base_path": "/api/v1",
                "response_format": "json",
                "error_handling": {
                    "use_global_exception_handler": true,
                    "standard_error_response": true
                }
            }
        },
        "llm_integration": {
            "providers": [
                "deepseek",
                "gemini"
            ],
            "api_config": {
                "timeout": 30000,
                "retry_attempts": 3,
                "rate_limiting": {
                    "requests_per_minute": 60
                }
            },
            "prompt_templates": {
                "image_analysis": "analyze_image_template.txt",
                "latex_generation": "generate_latex_template.txt"
            }
        },
        "pdf_generation": {
            "engine": "pdflatex",
            "templates": {
                "directory": "templates/latex",
                "default_template": "default.tex"
            },
            "output": {
                "directory": "generated_pdfs",
                "naming_pattern": "document_{timestamp}.pdf"
            },
            "compilation": {
                "timeout": 60000,
                "max_attempts": 3
            }
        }
    }
} 